{
  "name": "@healz/api",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "dotenv -e .env -- nest start --watch",
    "build": "nest build",
    "start": "node dist/main",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,test}/**/*.ts\"",
    "test": "jest",
    "test:e2e": "dotenv -e .env.test -- jest --config test/config/jest-e2e.json",
    "test:e2e:watch": "dotenv -e .env.test -- jest --config test/config/jest-e2e.json --watch",
    "test:e2e:cov": "dotenv -e .env.test -- jest --config test/config/jest-e2e.json --coverage",
    "test:e2e:debug": "dotenv -e .env.test -- node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand --config test/config/jest-e2e.json",
    "test:db:up": "docker compose -f docker-compose.test.yml up -d",
    "test:db:down": "docker compose -f docker-compose.test.yml down -v",
    "test:db:reset": "pnpm test:db:down && pnpm test:db:up && sleep 5 && dotenv -e .env.test -- pnpm db:push",
    "clean": "rm -rf dist",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "drizzle-kit migrate",
    "db:push": "drizzle-kit push",
    "db:studio": "drizzle-kit studio"
  },
  "dependencies": {
    "@healz/shared": "workspace:*",
    "@langchain/core": "^1.1.29",
    "@langchain/openai": "^1.2.11",
    "@nestjs/common": "^10",
    "@nestjs/config": "^3",
    "@nestjs/core": "^10",
    "@nestjs/jwt": "^11.0.2",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10",
    "@nestjs/swagger": "^11.2.6",
    "@nestjs/throttler": "^6.5.0",
    "amqp-connection-manager": "^5.0.0",
    "amqplib": "^0.10.9",
    "bcrypt": "^6.0.0",
    "better-auth": "^1.4.18",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "cookie-parser": "^1.4.7",
    "date-fns": "^4.1.0",
    "drizzle-orm": "^0.45.1",
    "langchain": "^1.2.28",
    "nanoid": "^5.1.6",
    "passport-jwt": "^4.0.1",
    "pg": "^8.18.0",
    "postgres": "^3.4.8",
    "reflect-metadata": "^0.2",
    "resend": "^6.9.1",
    "rxjs": "^7",
    "swagger-ui-express": "^5.0.1",
    "uuid": "^13.0.0",
    "zod": "^4.3.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^10",
    "@nestjs/schematics": "^10",
    "@nestjs/testing": "^10",
    "@types/amqplib": "^0.10.8",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.10",
    "@types/express": "^5.0.6",
    "@types/jest": "^29",
    "@types/node": "^22",
    "@types/passport-jwt": "^4.0.1",
    "@types/pg": "^8.16.0",
    "@types/supertest": "^6.0.3",
    "dotenv-cli": "^11.0.0",
    "drizzle-kit": "^0.31.8",
    "eslint": "^9",
    "jest": "^29",
    "supertest": "^7.2.2",
    "ts-jest": "^29",
    "ts-loader": "^9",
    "ts-node": "^10",
    "tsconfig-paths": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  }
}